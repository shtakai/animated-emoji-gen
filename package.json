{
  "name": "svg-sample",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint:fix": "eslint --fix 'src/**/*.{js,ts,vue}'",
    "format": "prettier 'src/**/*.{js,ts,vue}'",
    "format:fix": "prettier --write 'src/**/*.{js,ts,vue}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/gif.js": "^0.2.2",
    "bulma": "^0.9.3",
    "canvg": "^3.0.7",
    "detect-browser": "^5.2.0",
    "gif.js": "^0.2.0",
    "hex-rgb": "^5.0.0",
    "makerjs": "^0.17.1",
    "opentype.js": "^1.3.3",
    "sass": "^1.37.5",
    "text-to-svg": "^3.1.5",
    "vue": "^3.0.5",
    "vue-gtag": "^2.0.1",
    "vue-i18n": "^9.1.7",
    "vue3-colorpicker": "^1.0.5"
  },
  "devDependencies": {
    "@types/text-to-svg": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-vue": "7.15.1",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "2.3.2",
    "typescript": "^4.3.2",
    "vite": "^3.2.11",
    "vue-tsc": "^0.2.2"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "yarn format:fix",
      "yarn lint:fix"
    ]
  }
}
